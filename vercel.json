{
    "version": 2,
    "env": {
        "MONGO_URI": "@cooee_project1_uri"
    },
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ],
            "headers":{
                "Access-Control allow origin":"*"
            }
        }
    ]
}