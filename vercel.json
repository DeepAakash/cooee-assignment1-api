{
    "version": 2,
    "env": {
      "MONGO_URI": "@DB_URI",
      "JWT_SECRET": "@JWT_SECRET",
      "JWT_EXPIRES": "@JWT_EXPIRES"
    },
    "builds": [
      {
        "src": "src/main.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/main.ts",
        "methods": [
          "GET",
          "POST",
          "PUT",
          "DELETE"
        ]
      }
    ],
    "output": "dist"
  }
  